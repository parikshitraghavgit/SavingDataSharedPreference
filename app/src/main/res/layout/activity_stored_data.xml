<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".StoredData"
    android:orientation="vertical">

    <TextView
        android:id="@+id/textViewFirstName"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:hint="First Name Not Entered"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="22sp"></TextView>

    <TextView
        android:id="@+id/textViewLastName"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:hint="Last NAme Not Entered"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="22sp"></TextView>

    <TextView
        android:id="@+id/textViewEmail"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:hint="Email Not Entered"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="22sp"></TextView>

    <TextView
        android:id="@+id/textViewNumber"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:hint="Phone Number Not Entered"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="22sp"></TextView>

    <TextView
        android:id="@+id/textViewDob"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:hint="Date of Birth Not Entered"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="22sp"></TextView>

    <TextView
        android:id="@+id/textViewQualification"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:hint="Qualification Not Entered"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="22sp"></TextView>
</LinearLayout>